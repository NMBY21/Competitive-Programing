n = [1, 2, 6, 9, 2, 0, 2]
m = 2

first = -1
last = -1

for i in range(len(n)):
    if n[i] == m:
        if first == -1:
            first = i
        last = i

if first == -1:
    print(str(m) + " not found in the list.")
else:
    print("First index of " + str(m) + ": " + str(first))
    print("Last index of " + str(m) + ": " + str(last))
